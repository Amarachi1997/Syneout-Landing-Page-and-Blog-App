<%- include('partials/header') -%>
<form action="/compose" method="post">
    <div class="form-group">
        <label for="newtitle" class="compose-label">Post Title</label>
        <input type="text" name="postTitle" class="form-control">
        <label for="newPost" class="compose-label">Your Content</label>
        <textarea name="postContent" cols="40" rows="5" class="form-control"></textarea>
    </div>
    <button type="submit" name="button" class="btn btn-primary">Publish Post</button>
</form>

<%- include('partials/footer') -%>